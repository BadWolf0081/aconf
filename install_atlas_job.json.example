{
    "install atlas":
    [
        {
        "TYPE": "jobType.PASSTHROUGH",
        "SYNTAX": "su -c 'touch /data/local/tmp/aconf_download && echo https://mydownloadfolder.com > /data/local/tmp/aconf_download  && mount -o remount,rw /system && /system/bin/curl -L -o /system/bin/atlas.sh -k -s https://raw.githubusercontent.com/dkmur/aconf/master/atlas.sh && chmod +x /system/bin/atlas.sh && /system/bin/atlas.sh -ia'"
        }
    ]
}
